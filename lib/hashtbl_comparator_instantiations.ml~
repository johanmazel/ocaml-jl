



module BatHashtbl_comparator = Hashtbl_comparator.Make(BatHashtbl)


module Core_hashtbl_type = struct
  type ('a, 'b) t = ('a, 'b) Core_kernel.Core_hashtbl.t

  let find = Core_kernel.Core_hashtbl.find_exn

  let fold f t acc =
    Core_kernel.Core_hashtbl.fold
      t
      ~f: (fun ~key: key ~data: data acc ->
          f key data acc
        )
      ~init: acc
end

module Core_hashtbl_comparator = Hashtbl_comparator.Make(Core_hashtbl_type)
    
